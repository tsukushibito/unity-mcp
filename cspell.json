{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "version": "0.2",
    "language": "en",
    "words": [
        "protoc",
        "protobuf",
        "proto",
        "proto3",
        "grpc",
        "gRPC",
        "tonic",
        "prost",
        "serde",
        "serde_json",
        "tokio",
        "Tokio",
        "anyhow",
        "thiserror",
        "hyper",
        "tower",
        "tonic_build",
        "prost_build",
        "rustfmt",
        "clippy",
